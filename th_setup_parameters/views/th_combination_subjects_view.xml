<odoo>
    <record id="th_combination_subjects_view_tree" model="ir.ui.view">
        <field name="name">th_combination_subjects_view_tree</field>
        <field name="model">th.combination.subjects</field>
        <field name="arch" type="xml">
            <tree string="" editable="bottom">
                <field name="name"/>
                <field name="th_description"/>
            </tree>
        </field>
    </record>

    <record id="th_combination_subjects_view_act" model="ir.actions.act_window">
        <field name="name">Tổ hợp môn</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">th.combination.subjects</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>