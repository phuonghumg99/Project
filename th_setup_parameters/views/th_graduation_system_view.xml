<odoo>
    <record id="th_graduation_system_view_tree" model="ir.ui.view">
        <field name="name">th_graduation_system_view_tree</field>
        <field name="model">th.graduation.system</field>
        <field name="arch" type="xml">
            <tree string="" editable="bottom">
                <field name="name"/>
                <field name="th_description" required="1"/>
            </tree>
        </field>
    </record>

    <record id="th_graduation_system_view_act" model="ir.actions.act_window">
        <field name="name">Hệ tốt nghiệp</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">th.graduation.system</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>