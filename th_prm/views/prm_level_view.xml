<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="prm_level_tree_view" model="ir.ui.view">
        <field name="name">prm.level.tree</field>
        <field name="model">prm.level</field>
        <field name="arch" type="xml">
            <tree string="Mối quan hệ" editable="bottom">
                <field name="th_sequence" widget="handle"/>
                <field name="name"/>
                <field name="th_type"/>
                <field name="th_first_status"/>
                <field name="th_last_status"/>
                <field name="th_description"/>
            </tree>
        </field>
    </record>

    <record id="prm_level_action" model="ir.actions.act_window">
        <field name="name">Mối quan hệ</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">prm.level</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>