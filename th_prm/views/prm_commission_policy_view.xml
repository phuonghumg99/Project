<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="prm_commission_policy_view_tree" model="ir.ui.view">
        <field name="name">prm.commission.policy.tree</field>
        <field name="model">prm.commission.policy</field>
        <field name="arch" type="xml">
            <tree string="Chính sách hợp tác" editable="bottom">
                <field name="name"/>
                <field name="th_description"/>
            </tree>
        </field>
    </record>

    <record id="action_prm_commission_policy_view" model="ir.actions.act_window">
        <field name="name">Chính sách hợp tác</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">prm.commission.policy</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>