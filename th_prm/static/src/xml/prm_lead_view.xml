<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="th_prm.PrmLeadListView.Buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="primary" owl="1">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <button t-if="isManager" type="button" class="btn btn-primary ms-1" t-on-click="onClickSettingDomainLead">
                Thu hồi về kho
            </button>
        </xpath>
    </t>
    <t t-name="th_prm.PrmLeadListReuseView.Buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="primary" owl="1">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <button t-if="isManager" type="button" class="btn btn-primary ms-1" t-on-click="onClickReuseLead">
                Tái sử dụng
            </button>
        </xpath>
    </t>
</templates>