<odoo>
    <record id="th_trm_warehouse_view_tree" model="ir.ui.view">
        <field name="name">th_trm_warehouse_view_tree</field>
        <field name="model">th.trm.warehouse</field>
        <field name="arch" type="xml">
            <tree string="" editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="th_trm_warehouse_view_act" model="ir.actions.act_window">
        <field name="name">Kho giảng viên</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">th.trm.warehouse</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>